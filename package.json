{
  "name": "webrtc-videoconference",
  "version": "1.0.0",
  "description": "Video conferencing app with WebRTC and messaging",
  "main": "server.js",
  "scripts": {
    "start": "node server-react.js",
    "start:ts": "ts-node server-react.ts",
    "start-old": "node server.js",
    "dev": "nodemon server-react.js",
    "dev:ts": "nodemon --exec \"ts-node\" server-react.ts",
    "dev-old": "nodemon server.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "client-type-check": "cd client && npm run type-check",
    "client-lint": "cd client && npm run lint",
    "client-lint:fix": "cd client && npm run lint:fix",
    "install-all": "npm install && npm run client-install",
    "build": "npm run client-type-check && npm run client-build",
    "build:ts": "tsc && npm run client-build",
    "build:prod": "cd client && npm run build:prod",
    "type-check": "npm run client-type-check",
    "type-check:backend": "tsc --noEmit",
    "type-check:all": "npm run type-check:backend && npm run client-type-check",
    "lint": "npm run client-lint",
    "lint:fix": "npm run client-lint:fix",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "dev:full:ts": "concurrently \"npm run dev:ts\" \"npm run client\"",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "webrtc",
    "video",
    "conference",
    "messaging",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-ical": "^0.16.1",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
